{
  "name": "pitta-blog-dio",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run start:all",
    "start:all": "npm-run-all --parallel start:client start:server",
    "start:client": "ng serve",
    "start:server": "npm --prefix src/assets/server run start",
    "install:all": "npm run install:server & npm run install:client",
    "install:client": "npm i -f",
    "install:server": "npm --prefix src/assets/server i -f",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.5",
    "@angular/cdk": "^18.2.5",
    "@angular/common": "^18.2.5",
    "@angular/compiler": "^18.2.5",
    "@angular/core": "^18.2.5",
    "@angular/forms": "^18.2.5",
    "@angular/localize": "^18.2.5",
    "@angular/material": "^18.2.5",
    "@angular/platform-browser": "^18.2.5",
    "@angular/platform-browser-dynamic": "^18.2.5",
    "@angular/router": "^18.2.5",
    "@ng-bootstrap/ng-bootstrap": "^13.0.0",
    "@popperjs/core": "^2.10.2",
    "bootstrap": "^5.2.0",
    "ngx-quill": "^26.0.8",
    "npm-run-all": "^4.1.5",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "uui": "^1.0.7",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.5",
    "@angular/cli": "~18.2.5",
    "@angular/compiler-cli": "^18.2.5",
    "@types/ckeditor__ckeditor5-build-classic": "^37.0.0",
    "@types/jasmine": "~4.0.0",
    "@types/uuid": "^10.0.0",
    "jasmine-core": "~4.3.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~5.4.5"
  }
}
